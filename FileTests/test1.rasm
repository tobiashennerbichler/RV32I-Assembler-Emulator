_start:
        addi    sp, zero, 0x700
        JAL     ra, main
        EBREAK

square:
        addi    sp, sp, -16
        sw      ra, 12(sp)
        sw      fp, 8(sp)
        addi    fp, sp, 16
        sw      a0, -12(fp)
        lw      a0, -12(fp)
        add     a0, a0, a0
        lw      fp, 8(sp)
        lw      ra, 12(sp)
        addi    sp, sp, 16
        jalr    zero, 0(ra)

main:
        addi    sp, sp, -32
        sw      ra, 28(sp)
        sw      fp, 24(sp)
        addi    fp, sp, 32
        add     a0, zero, zero
        sw      a0, -20(fp)
        sw      a0, -12(fp)
        addi    a0, zero, 3
        sw      a0, -16(fp)
        lw      a0, -16(fp)
        jal     ra, square
        add     a1, a0, zero
        lw      a0, -20(fp)
        sw      a1, -16(fp)
        lw      fp, 24(sp)
        lw      ra, 28(sp)
        addi    sp, sp, 32
        jalr    zero, 0(ra)